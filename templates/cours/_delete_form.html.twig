<form method="post" action="{{ path('cours_delete', {'id': cour.id}) }}" onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ cour.id) }}">
    <button class="btn btn-danger" style="margin-top:15px;">Supprimer</button>
</form>
